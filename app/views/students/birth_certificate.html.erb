<center>
    <h1>Birth Certificate</h1>
    <h3><%= @student.proper_name %></h3>
    <iframe src="<%= @student.birth_certificate_url %>" width="100%" height="400px"></iframe>
    <div>
        <%= link_to "Checkoff", birth_certificate_checkoff_student_path(@student) %>
        <%= link_to "Deny", birth_certificate_deny_student_path(@student) %>
    </div>
</center>
