<div class="row">
  <div class="col-xs-12 col-sm-12">
    <center>
      <%= link_to image_tag('boblogo.png'), home_path %><br>
      <h1>Project C.H.U.C.K. 2015</h1>
    </center>
  </div>
</div>
<hr>

<div class = "well">

<center><h2>Log In</h2> <br>

<%= form_tag sessions_path do %>


<div class="control-group">
  <%= label_tag :username %>
  <div class="controls">
    <%= text_field_tag :username, params[:username] %>
  </div>
</div>

<div class="control-group">
  <%= label_tag :password %>
  <div class="controls">
    <%= password_field_tag :password %>
  </div>
</div>

<div class="actions"><%= submit_tag "Log In" %></div> </center>
<% end %>

</div>