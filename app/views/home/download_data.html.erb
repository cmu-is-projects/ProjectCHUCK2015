<center><h3>Students</h3></center>
<table class="table">
    <thead>
        <tr>
            <th>Student Name</th>
            <th>Guardian Name</th>
            <th>Guardian Email</th>
            <th>Street</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>County</th>
        </tr>
    </thead>
    <tbody>
      <% @students.each do |student| %>
          <tr>
            <td><%= student['first_name'] + " " + student['last_name'] %></td>
            <td><%= student['g_first_name'] + " " + student['g_last_name'] %></td>
            <td><%= student['g_email'] %></td>
            <td><%= student['h_street'] %></td>
            <td><%= student['h_city'] %></td>
            <td><%= student['h_state'] %></td>
            <td><%= student['h_zip'] %></td>
            <td><%= student['h_county'] %></td>
          </tr>
      <% end %>
    </tbody>
</table>
