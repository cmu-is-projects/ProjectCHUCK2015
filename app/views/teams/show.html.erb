<center>
  <p id="notice"><%#= notice %></p>
  <div class="row">
    <div class="col-sm-12">

      <h2>
        <!-- <strong>Name:</strong> -->
        <b><%= @team.name %></b>
      </h2>

      <h3>
        <!-- <strong>Bracket:</strong> -->
        <%= @team.bracket.gender + " " + @team.bracket.min_age.to_s + "-" + @team.bracket.max_age.to_s %> age bracket
      </h3>

      <h3>
        <!-- <strong>Spots Filled:</strong> -->
        <%= @team.roster_spots.count.to_s + " out of " + @team.max_students.to_s %> spots filled
      </h3> 

    </div>
  </div>
  <div class="noprint">
  <%= link_to(image_tag('edit.png'), edit_team_path(@team)) %> Edit | 
  <%= link_to(image_tag('back16.png'), teams_path) %> Back
</div>
</center> <br>

<div class="row">
  <div class="col-sm-6">
    <%= render :partial => 'volunteer_fields' %>
  </div>
</div>
<div class ="row">
  <div class="col-sm-6">
    <%= render :partial =>  'student_details' %>
  </div>
</div>

