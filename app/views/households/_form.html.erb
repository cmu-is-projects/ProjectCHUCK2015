<%= form_for(@household) do |f| %>
<% if @household.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@household.errors.count, "error") %> prohibited this household from being saved:</h2>

  <ul>
    <% @household.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="container">

  <div class="row">
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :street %>
        <%= f.text_field :street %>
      </div>
    </div>
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :city %>
        <%= f.text_field :city %>
      </div>
    </div>
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :zip %>
        <%= f.text_field :zip %>
      </div>
    </div>
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :county %>
        <%= f.text_field :county %>
      </div>
    </div>
    <div class="col-xs-6 col-sm-4">
      <div class="field">
        <%= f.label :home_phone %>
        <%= f.text_field :home_phone %>
      </div>
    </div>
  </div>

<div class="actions">
  <%= f.submit %>
</div>
</div>
<!-- <div class="field">
  <%#= f.label :active %><br>
  <%#= f.check_box :active %>
</div> -->

<% end %>
